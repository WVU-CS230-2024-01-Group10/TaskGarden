<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" contebt="width=device-width, initial-scale=1.0">
    <title>Website Navigation</title>
</head>
<body>
    <nav>
        <ul>
            
            <li><a href="#" onclick="navigateToPage('home')">Home</a></li>
            <li><a href="#" onclick="navigateToPage('loginPage')">Loginpage</a></li>
            <li><a href="#" onclick="navigateToPage('taskPage')">Taskpage</a></li>
            <li><a href="#" onclick="navigateToPage('studyPage')">Studypage</a></li>
            <li><a href="#" onclick="navigateToPage('GreenhousePage')">Greenhouse-page</a></li>
        </ul>
    </nav>

    <div id="content">
        <!-- Content of the current page will be loaded here -->
    </div>


    <script>
        function navigateToPage(page) {
            switch (page) {
                case 'home':
                    document.getElementById('content').innerHTML = '<h2>Welcome to the Homepage!</h2>';
                    break;
                case 'loginPage':
                    document.getElementById('content').innerHTML = '<h2>Welcome to the Loginpage!</h2>';
                    break;
                case 'taskPage':
                    document.getElementsById('content').innerHTML = '<h2>Welcome to the Taskpage!</h2>';
                    break;
                case 'studyPage':
                    document.getElementsById('content').innerHTML = '<h2>Welcome to the Studypage!</h2>';
                    break;
                case 'GreenhousePage':
                    document.getElementsById('content').innerHTML = '<h2>Welcome to the Greenhouse-page!</h2>';
                    break;
                defualt:
                    document.getElementById('content').innerHTML = '<h2>Page Not Found</h2>';
            }

        }
    </script>
    </body>
    </html>
